{
  "name": "texter",
  "version": "2.1.0",
  "repository": "https://github.com/ianjsikes/texter",
  "private": true,
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "firebase": "^8.6.1",
    "firebase-rxjs": "^0.1.2",
    "font-awesome": "^4.7.0",
    "history": "^5.0.0",
    "immer": "^9.0.2",
    "json2csv": "^5.0.6",
    "mongodb": "^3.6.7",
    "papaparse": "^5.3.0",
    "phone-formatter": "^0.0.2",
    "ramda": "^0.27.1",
    "react": "^17.0.2",
    "react-chat-ui": "^0.4.0",
    "react-dom": "^17.0.2",
    "react-file-reader": "^1.1.4",
    "react-fontawesome": "^1.7.1",
    "react-redux": "^7.2.4",
    "react-router": "^5.2.0",
    "react-router-redux": "^4.0.8",
    "react-scripts": "^4.0.3",
    "rebass": "^4.0.7",
    "redux": "^4.1.0",
    "redux-saga": "^1.1.3",
    "rxjs": "^7.0.1",
    "serve": "^11.3.2",
    "string-template": "^1.0.0",
    "styled-components": "^5.3.0",
    "styled-system": "^5.1.5",
    "twilio": "^3.62.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "now-start": "serve --auth --single ./build",
    "deploy": "now --public -e SERVE_USER=@serve-user -e SERVE_PASSWORD=@serve-password && now alias \"$(pbpaste)\" texter-web",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "now": {
    "name": "texter-web",
    "alias": "texter-web",
    "public": true,
    "env": {
      "SERVE_USER": "@serve-user",
      "SERVE_PASSWORD": "@serve-password"
    }
  },
  "devDependencies": {
    "@types/mongodb": "^3.6.14",
    "@vercel/node": "^1.10.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.0"
  }
}
